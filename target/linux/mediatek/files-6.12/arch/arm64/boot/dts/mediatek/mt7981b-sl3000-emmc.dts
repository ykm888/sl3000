/dts-v1/;

#include "mediatek/mt7981.dtsi"
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
    compatible = "sl3000,emmc", "mt7981b-sl3000-emmc", "mediatek,mt7981b";
    model = "SL-3000 eMMC Router";

    memory@40000000 {
        device_type = "memory";
        reg = <0x0 0x40000000 0x0 0x40000000>; /* 1GB RAM */
    };
};
